<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="oval">
            <solid android:color="@color/md_gray_200"/>
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
    <item android:left="4dp" android:right="4dp" android:bottom="4dp" android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="@color/md_purple_300"/>
            <size
                android:width="10dp"
                android:height="10dp"/>
        </shape>
    </item>
    <item android:width="10dp" android:height="10dp"
        android:top="10dp" android:bottom="10dp" android:left="10dp" android:right="10dp"
        android:gravity="center"
        android:drawable="@drawable/phone_icon">
    </item>
</layer-list>